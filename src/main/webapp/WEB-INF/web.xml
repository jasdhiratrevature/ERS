    <display-name>EmployeeController</display-name>
    <servlet-name>EmployeeController</servlet-name>
    <servlet-class>com.jasdhir.ers.controllers.EmployeeController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EmployeeController</servlet-name>
    <url-pattern>/EmpC</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ManagerController</display-name>
    <servlet-name>ManagerController</servlet-name>
    <servlet-class>com.jasdhir.ers.controllers.ManagerController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ManagerController</servlet-name>
    <url-pattern>/ManC</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>EmpRemReq</display-name>
    <servlet-name>EmpRemReq</servlet-name>
    <servlet-class>com.jasdhir.ers.controllers.EmpRemReq</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EmpRemReq</servlet-name>
    <url-pattern>/EmpReq</url-pattern>
  </servlet-mapping>
</web-app>